unit SolarizedColorUnit ;

interface

  uses
    Winapi.Windows ,
    System.SysUtils ,
    System.Classes ,
    Vcl.Graphics ;

  type
    TSolarised = class

      private
        fBase03 : TColor ;

      public
        property base03 : TColor read fBase03 ;


    end ;

implementation

end.
